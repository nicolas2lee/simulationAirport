public class Airplane extends SimEntity {

	private String name;
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Airplane(SimEngine engine) {
		super(engine);
	}
	
	/*
	public void sayHelloTo(Airplane other) {
		System.out.println("[" + name + "] Hello " + other + " !");
	}
*/
	@Override
	public String toString() {
		return name;
	}
	
	
	
}