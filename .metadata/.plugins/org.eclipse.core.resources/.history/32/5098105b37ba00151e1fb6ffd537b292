		this.salonFeatures = salonFeatures;
		this.salonInit = salonInit;
		this.ArrivalDelayRecordingCatGen = arrivalDelayRecordingCatGen;
		this.delaiAttenteRecordingCatGen = delaiAttenteRecordingCatGen;
	}
	/**
