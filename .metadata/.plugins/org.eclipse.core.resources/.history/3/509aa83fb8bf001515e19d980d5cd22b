package enstabretagne.SimEntity.airplane;

public enum StateAirplane {
	Arrive("Arrive"),
	NotifyBeginArrive("NotifyBeginArrive"),
	WaitForFIFO("WaitForFIFO"),
	CloseToAirport("CloseToAirport"),
	Landed("Landed"),
	RolledToGate("RolledToGate"),
	NotifiedEndArrive("NotifiedEndArrive"),
	UnloadedPassagers("UnloadedPassagers"),
	PreparedToLoading("PreparedToLoading"),
	LoadedPassagers("LoadedPassagers"),
	NotifiedBeginDepart("NotifiedBeginDepart"),
	
	Depart("Depart");
	
	
	private String StateAirplane;
	private StateAirplane(String state){
		this.StateAirplane=state;
	}

}
